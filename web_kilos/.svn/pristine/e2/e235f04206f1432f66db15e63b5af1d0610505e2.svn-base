<?xml version="1.0" encoding="UTF-8"?>
<jeus-web-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.tmaxsoft.com/xml/ns/jeus http://www.tmaxsoft.com/xml/ns/jeus/jeus-web-dd.xsd">
    <context-path>/kilos</context-path>
    <auto-reload>
        <enable-reload>true</enable-reload>
        <check-on-demand>false</check-on-demand>
    </auto-reload>
    <jsp-engine>
        <compile-option>-g</compile-option>
    </jsp-engine>
    <fast-deploy>true</fast-deploy>
</jeus-web-dd>